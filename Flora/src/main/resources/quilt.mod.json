{
	"schema_version": 1,
	"quilt_loader": {
		"group": "${group}",
		"id": "diversity-flora",
		"version": "${version}",
		"intermediate_mappings": "net.fabricmc:intermediary",
		"metadata": {
			"name": "Diversity: Flora",
			"description": "Diversity module that adds more flora to your world",
			"contributors": {
				"ENDERZOMBI102": "Owner"
			},
			"contact": {
				"sources": "https://github.com/ENDERZOMBI102/Diversity",
				"issues": "https://github.com/ENDERZOMBI102/Diversity/issues"
			},
			"license": "MIT",
			"icon": "assets/diversity-flora/flora.png"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": "${loader_version}"
			},
			{
				"id": "quilt_base",
				"versions": "*"
			},
			{
				"id": "minecraft",
				"versions": ">=${minecraft_version}"
			},
			{
				"id": "java",
				"versions": ">=17"
			}
		],
		"entrypoints": {
			"init": [
				"com.enderzombi102.diversity.flora.Flora"
			],
			"client_init": [
				"com.enderzombi102.diversity.flora.FloraClient"
			]
		}
	},
	"minecraft": {
		"environment": "*"
	},
	"mixin": "diversity-flora.mixins.json",
	"modmenu": {
		"parent": {
			"id": "diversity",
			"name": "Diversity",
			"description": "Diversity is a mod all about the nature ecosystems.",
			"icon": "assets/diversity/icon.png"
		}
	}
}