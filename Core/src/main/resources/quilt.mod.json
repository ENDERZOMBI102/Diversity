{
	"schema_version": 1,
	"quilt_loader": {
		"group": "${group}",
		"id": "diversity-core",
		"version": "${version}",
		"intermediate_mappings": "net.fabricmc:intermediary",
		"metadata": {
			"name": "Diversity: Core",
			"description": "Core module for Diversity.",
			"contributors": {
				"ENDERZOMBI102": "Owner"
			},
			"contact": {
				"sources": "https://github.com/ENDERZOMBI102/Diversity",
				"issues": "https://github.com/ENDERZOMBI102/Diversity/issues"
			},
			"license": "MIT",
			"icon": "assets/diversity-core/icon.png"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": "${loader_version}"
			},
			{
				"id": "quilt_base",
				"versions": "*"
			},
			{
				"id": "minecraft",
				"versions": ">=${minecraft_version}"
			},
			{
				"id": "java",
				"versions": ">=17"
			},
			{
				"id": "cloth-config2",
				"versions": ">=${cc_version}"
			}
		],
		"entrypoints": {
			"init": [
				"com.enderzombi102.diversity.core.Core"
			],
			"client_init": [
				"com.enderzombi102.endconfig.impl.EndConfigImpl"
			],
			"modmenu": [
				"com.enderzombi102.diversity.core.ModmenuCompat"
			]
		}
	},
	"minecraft": {
		"environment": "*"
	},
	"mixin": [
		"diversity-core.mixins.json",
		"endconfig.mixins.json",
		"enderlib.mixins.json"
	],
	"modmenu": {
		"parent": {
			"id": "diversity",
			"name": "Diversity",
			"description": "Diversity is a mod all about the nature ecosystems.",
			"icon": "assets/diversity/icon.png"
		}
	}
}